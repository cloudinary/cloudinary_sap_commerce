			<bean class="uk.ptr.cloudinary.conv.Oauth2AccessTokenConverter" />
		</property>
	</bean>
	<bean class="de.hybris.platform.commercefacades.xstream.conv.TypeConverterMapping">
		<property name="converter">
			<bean class="uk.ptr.cloudinary.conv.SearchQueryDataConverter">
